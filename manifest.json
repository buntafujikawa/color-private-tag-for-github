{
  "name": "Color Private Tag for GitHub",
  "description": "Change private tag in GitHub to color of repository language.",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "script.js"
      ]
    }
  ],
  "permissions": [
    "https://github.com/*"
  ]
}
